{
  "name": "aline-transaction-generator",
  "version": "0.1.0",
  "description": "Generate realistic bank account transactions for the Aline Financial REST API",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist/",
    "copy-files": "copyfiles -u 1 src/**/*.html src/public/**/*.js dist/",
    "build": "tsc",
    "prestart": "npm run clean && npm run build && npm run copy-files",
    "start": "node .",
    "start:dev": "ts-node-dev --respawn ./src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "leandroyabut",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.41",
    "axios": "^0.27.2",
    "dayjs": "^1.11.3",
    "express": "^4.18.1",
    "tslog": "^3.3.3"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.3.0",
    "copyfiles": "^2.4.1",
    "rimraf": "^3.0.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.4"
  }
}
